population=rchisq(10000000,2)
hist(population)
mean(population)
sd(population)

sample=rnorm(50,mean(population),sd(population))
hist(sample)
mean(sample)



xbarholder=numeric(10000)
xbar=mean(sample)


for (i in 1:10000)
    sample=rnorm(50,mean(population),sd(population))
    xbar=mean(sample)
    xbarholder[i]=xbar
    
xbarholder
